{
  "rules": {
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "calendars": {
      "$calendar_id": {
      ".read": "auth !=null && data.child('permissionRead').hasChild(root.child('users/'+auth.uid+'))",
      ".write": "auth !=null && data.child('permissionWrite').hasChild(root.child('users/'+auth.uid+'))"
      }
    }
  } 
}

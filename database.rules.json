{
  "rules": {
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "calendars": {
      "$calendar_id": {
      ".read": "auth !=null && root.child('users/'+auth.uid+'/checkit_allCalendarIds/'+data.child('uniqueId').val()).exists()",
      ".write": "auth !=null && root.child('users/'+auth.uid+'/checkit_allCalendarIds/'+data.child('uniqueId').val()).exists()"
      }
    }
  } 
}

{
  "rules": {
    "users": {
      "$uid": {
        ".read" : "auth!== null && auth.uid === $uid",
        ".write" : "auth !== null && auth.uid === $uid"
      }
    },
    "calendars":{
      "$calendarId": {
        "calendarState": {
          ".read" : "auth!== null && data.parent().child('read').hasChild(auth.uid) && data.parent().child('read').child(auth.uid).val() === true",
          ".write" : "auth !== null && data.parent().child('write').hasChild(auth.uid) && data.parent().child('write').child(auth.uid).val() === true"
        },
        "write" : {
          ".read" :,
          ".write" :
        }
        "read" : {
          ".read" :,
          ".write" :
        }
      }
    }
  } 
}
